<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="${relativePackage}.${activityClass}"
    android:background = "@drawable/tipsytesthome"
    >

    <Button
        android:id="@+id/faqPage"
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:background = "@drawable/faqbutton"
        android:soundEffectsEnabled = "false"
        style="?android:attr/borderlessButtonStyle" 
        />
 
    <Button
        android:id="@+id/nextPage"
        android:layout_width="350dp"
        android:layout_height="90dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/beginbutton"
        android:soundEffectsEnabled = "false" />

    <Button
        android:id="@+id/contactPage"
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:background="@drawable/contactusbutton"
        style="?android:attr/borderlessButtonStyle" 
        android:soundEffectsEnabled = "false"/>

</RelativeLayout>