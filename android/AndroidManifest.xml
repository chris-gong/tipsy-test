<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.crazystupidgames.tipsytest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/applogo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".HomepageActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FirstGameActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_first_game"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".SecGameActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_sec_game"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".ThirdGameActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_third_game"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".ThirdGameContActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_third_game_cont"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".FourthGameActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_fourth_game"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".FifthGameActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_fifth_game"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".ReactionTimesActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_reaction_times"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".FAQsActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_faqs"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".ContactUsActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_contact_us"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".FinalPageActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_final_page"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>
